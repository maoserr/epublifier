{
  "compilerOptions": {
    "allowSyntheticDefaultImports":true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "esnext",
    "module": "esnext",
    "strict": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowJs": true,
    "experimentalDecorators": true
  },
  "baseUrl": "./src",
  "include": ["./src"],
  "exclude": ["node_modules"]
}
